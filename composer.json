{
    "name": "tejadong/datatables-bundle",
    "description": "DataTables.js server-side processing for Doctrine Entities",
    "keywords": ["TejadongDatatables","Tejadong","DataTables"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Sikorra (author owner)",
            "email": "Chad.Sikorra@gmail.com"
        },
        {
          "name": "Jesus Tejada (author modifier)",
          "email": "tejadong@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~4.0||~5.0",
        "doctrine/orm": "~2.7",
        "doctrine/doctrine-bundle": "~2.1"
    },
    "suggest": {
        "jms/serializer-bundle": "Advanced JSON encoding capabilities, recommended."
